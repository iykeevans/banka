{
  "name": "banka-server",
  "version": "1.0.0",
  "description": "Banka is a light-weight core banking application that powers banking operations like account\r creation, customer deposit and withdrawals.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf bundle",
    "build-babel": "babel -d ./bundle ./server -s",
    "build": "npm run clean && npm run build-babel",
    "test": "cross-env NODE_ENV=Test npm run migrate && cross-env NODE_ENV=Test nyc mocha test/index.js --exit --require @babel/register --timeout 25000",
    "dev": "cross-env NODE_ENV=Development nodemon --exec babel-node server",
    "start": "npm run build && node bundle",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "seed": "babel-node server/models/config/seed.js",
    "migrate": "babel-node server/models/config/migration.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iykeevans/banka.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iykeevans/banka/issues"
  },
  "homepage": "https://github.com/iykeevans/banka#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "joi-date-extensions": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.3",
    "moment": "^2.24.0",
    "nodemailer": "^6.0.0",
    "pg-promise": "^8.6.5",
    "shortid": "^2.2.14",
    "ng-faker": "^1.1.0"
  }
}
